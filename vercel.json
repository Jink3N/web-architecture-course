{
  "public": true,
  "outputDirectory": "public",
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.1.5"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}